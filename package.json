{
  "name": "dotnet-core-commands",
  "displayName": "Dotnet core commands",
  "description":
    "Dotnet core SKD commands, EntityFrameworkCore migrations and NuGet Package management",
  "license": "MIT License",
  "version": "1.0.5",
  "author": "Matija Ram≈°ak",
  "publisher": "matijarmk",
  "engines": {
    "vscode": "^1.20.0"
  },
  "keywords": [
    "C#", 
    "core",
    "csharp",
    "NuGet",
    "Entity Framework",
    ".net"    
  ],
  "icon": "images/Logo.png",
  "galleryBanner": {
    "color": "white",
    "theme": "light"
  },
  "categories": ["Other"],
  
  "activationEvents": ["onCommand:extension.Dotnet"],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.Dotnet",
        "title": "dotnet"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "bugs": {
    "url": "https://github.com/matijarmk/dotnet-core-commands/issues",
    "email": "matijarmk@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matijarmk/dotnet-core-commands"
  },
  "homepage":
    "https://github.com/matijarmk/dotnet-core-commands/blob/master/README.md",
  "devDependencies": {
    "typescript": "^2.7.1",
    "vscode": "^1.1.10",
    "@types/node": "^9.4.5",
    "@types/mocha": "^2.2.48"
  },
  "dependencies": {
    "@types/xml-js": "^1.0.0",
    "file-system": "^2.2.2",
    "run-in-terminal": "^0.0.3",
    "web-request": "^1.0.7",
    "xml-js": "^1.6.2"
  }
}
